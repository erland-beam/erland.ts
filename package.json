{
  "name": "erland.ts",
  "version": "0.1.0",
  "description": "TypeScript client for Erland",
  "type": "module",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js",
      "main": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "author": "meppu <npm@meppu.boo>",
  "license": "MIT",
  "keywords": [
    "erland",
    "websocket",
    "beam",
    "erlang",
    "elixir"
  ],
  "scripts": {
    "build": "tsup src/index.ts",
    "watch": "tsup src/index.ts --watch",
    "test": "vitest --run",
    "prettier": "prettier -c .",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@types/ws": "^8.5.4",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  },
  "dependencies": {
    "ws": "^8.13.0"
  },
  "engines": {
    "node": ">=16.20"
  }
}
